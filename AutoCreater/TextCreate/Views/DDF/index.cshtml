<el-container>
    <el-header>
        <el-form v-bind:inline="true" v-bind:model="SearchForm">
                        <el-form-item label="X姓名">
                            <el-input v-model="SearchForm.XName" placeholder="X姓名"></el-input>
                        </el-form-item>
                        <el-form-item label="DDDL">
