                    <el-table-column prop="XName" label="X姓名" width="180" show-overflow-tooltip>
                    </el-table-column>
                    <el-table-column prop="DDDL" label="DDDL" width="180" show-overflow-tooltip>
                    </el-table-column>

                        <el-table-column prop="Name" label="Name" width="180" show-overflow-tooltip>
                        </el-table-column>
                        <el-table-column prop="Addr" label="Addr" width="180" show-overflow-tooltip>
                        </el-table-column>
                        <el-table-column prop="Age" label="Age" width="180" show-overflow-tooltip>
                        </el-table-column>
        </el-grid>
    </el-main>
</el-container>






